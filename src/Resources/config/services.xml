<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="false" />
        <service id="Kematjaya\SaleBundle\Service\SaleService" class="Kematjaya\SaleBundle\Service\SaleService">
            <argument type="service" id="doctrine.orm.default_entity_manager" />
            <argument type="service" id="kematjaya.stock_service" />
        </service>
        <service id="kematjaya.sale_service" alias="Kematjaya\SaleBundle\Service\SaleService" public="true"></service>
        <service id="Kematjaya\SaleBundle\Repo\SaleRepoInterface" alias="kematjaya.sale_repo" public="true"/>
        <service id="Kematjaya\SaleBundle\Repo\SaleItemRepoInterface" alias="kematjaya.sale_item_repo" public="true"/>
        <service id="Kematjaya\SaleBundle\Repo\CustomerRepoInterface" alias="kematjaya.customer_repo" public="true"/>
        <service id="Kematjaya\SaleBundle\EventSubscriber\SaleEventSubscriber" class="Kematjaya\SaleBundle\EventSubscriber\SaleEventSubscriber">
            <argument type="service" id="kematjaya.sale_service" />
        </service>
        <service id="Kematjaya\SaleBundle\EventSubscriber\SaleEventSubscriber\SaleItemFormEventSubscriber" class="Kematjaya\SaleBundle\EventSubscriber\SaleEventSubscriber\SaleItemFormEventSubscriber"></service>
        <service id="Kematjaya\SaleBundle\Form\Type\HiddenDateTimeType" class="Kematjaya\SaleBundle\Form\Type\HiddenDateTimeType">
            <tag name="form.type"/>
        </service>
        <!--
        <service id="Kematjaya\PurchashingBundle\EventSubscriber\PurchaseEventSubscriber" class="Kematjaya\PurchashingBundle\EventSubscriber\PurchaseEventSubscriber">
            <argument type="service" id="kematjaya.purchase_service" />
        </service>
        <service id="Kematjaya\PurchashingBundle\EventSubscriber\PurchaseFormEventSubscriber" class="Kematjaya\PurchashingBundle\EventSubscriber\PurchaseFormEventSubscriber">
            <argument type="service" id="property_info" />
        </service>-->
    </services>
</container>