        "symfony/event-dispatcher": "^4.0|^5.0",
        "symfony/http-kernel": "^4.0|^5.0",
        "symfony/security-core": "^4.0|^5.0",
        "fzaninotto/faker": "^1.9",
        "symfony/contracts": "^2.1"
